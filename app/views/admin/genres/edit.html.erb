<main>
  <div class="conteiner">
    <div class="row">
      <div class="col">
　　　　<h5 class="bg-secondary d-inline p-2">
          ジャンル編集
        </h5>
        <%= form_with model:@genre, url: admin_genre_path(@genre.id), local:true do |f| %>
          <%= f.label :ジャンル名 %>
          <%= f.text_field :name %>
          <%= f.submit "変更を保存", class:"btn btn-success" %>
        <% end %>
      </div>
    </div>
  </div>
</main>